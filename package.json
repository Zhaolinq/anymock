{
  "name": "@tiandatong/anymock",
  "version": "0.0.0-development",
  "description": "mock service base on Anyproxy.",
  "main": "lib/anymock.js",
  "bin": "lib/anymock.js",
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "jest --testMatch \"<rootDir>/test/*.unittest.js\"",
    "semantic-release": "semantic-release",
    "precommit": "lint-staged",
    "travis-deploy-once": "travis-deploy-once"
  },
  "lint-staged": {
    "lib/**/*.js": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/temberature/anymock.git"
  },
  "keywords": [
    "['mock'",
    "'Anyproxy'",
    "'proxy'",
    "'API'",
    "'HTTP'",
    "'simulator']"
  ],
  "author": "Tong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/temberature/anymock/issues"
  },
  "homepage": "https://github.com/temberature/anymock#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^2.1.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "pre-commit": "^1.2.2",
    "semantic-release": "^15.10.8",
    "travis-deploy-once": "^5.0.9"
  },
  "dependencies": {
    "anyproxy": "^4.0.12",
    "ramda": "^0.25.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
